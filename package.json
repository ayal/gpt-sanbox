{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "vite",
    "build:pure": "vite build --base=./",
    "lint:es": "yarn eslint ./src/index.ts",
    "tcheck": "yarn tsc --noEmit",
    "lint": "yarn tcheck && yarn lint:es",
    "build": "yarn lint && yarn build:pure",
    "build:core": "yarn build:pure"
  },
  "dependencies": {
    "@types/node": "^18.15.3",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "vite": "^4.1.4",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
